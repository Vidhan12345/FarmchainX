# Environment Variables Template
# Copy this file to .env and update with your actual values
# Note: .env files are ignored by git for security

# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/farmchainx
DB_USERNAME=farmchainx_user
DB_PASSWORD=your_secure_password

# JWT Configuration
# Generate a secure secret key (at least 256 bits)
# You can use: openssl rand -base64 32
FARMCHAINX_JWT_SECRET=YourSecureJWTSecretKeyHereThatShouldBeAtLeast256BitsLong

# Server Configuration (optional)
PORT=8080

# Development/Production Flag
SPRING_PROFILES_ACTIVE=dev
